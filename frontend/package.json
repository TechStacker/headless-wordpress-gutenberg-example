{
  "name": "@headless-wp/frontend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "private": true,
  "scripts": {
    "dev": "node .",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node .",
    "format": "prettier --write \"{pages,components,types}/**/*.{ts,tsx}\" && npm run lint:fix",
    "lint": "tslint '{pages,components,types}/**/*.{ts,tsx}'",
    "lint:fix": "tslint --fix '{pages,components,types}/**/*.{ts,tsx}'",
    "docker:build": "docker build -t frontend .",
    "docker:clean": "docker rm -f frontend || true",
    "docker:run": "docker run -p 3000:3000 --name frontend frontend",
    "docker:stop": "docker stop frontend",
    "docker:start": "docker start frontend && yarn run docker:logs",
    "docker:logs": "docker logs -f frontend",
    "deploy": "yarn run docker:build && yarn run docker:clean && yarn run docker:run"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-2.0",
  "browserslist": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@zeit/next-sass": "^0.1.2",
    "@zeit/next-typescript": "^0.1.1",
    "autoprefixer": "^8.4.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "isomorphic-unfetch": "^2.0.0",
    "next": "^6.0.0",
    "node-sass": "^4.9.0",
    "normalize.css": "^8.0.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.4",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "sass-loader": "^7.0.1",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/next": "^2.4.9",
    "@types/node": "^10.0.0",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "@types/styled-jsx": "^2.2.3",
    "cross-env": "^5.1.4",
    "prettier": "^1.12.1",
    "tslint": "^5.10.0",
    "tslint-config-blvd": "github:blvdgroup/tslint-config-blvd"
  }
}
